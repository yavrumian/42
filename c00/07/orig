#include <unistd.h>
#include <limits.h>

void ft_putnbr(int nb){
	int int_max = INT_MAX;
	int nbytes = 0;
	while(1){
		int_max = int_max/ 10;
		if(int_max < 1){
			break;
		}
		nbytes++;
	}
	char out[nbytes+1];
	int n = 0;
	while(1){
		out[nbytes-n] = nb%10 + '0';
		nb /= 10;
		if(nb < 1 && nb > -1){
			break;
		}
		n++;
	}
	if(nb < 0){
		out[nbytes - n - 1] = '-';
	}
	write(1, &out, nbytes+1);
}

int main(){
	ft_putnbr(-1220);
	return 0;
}
